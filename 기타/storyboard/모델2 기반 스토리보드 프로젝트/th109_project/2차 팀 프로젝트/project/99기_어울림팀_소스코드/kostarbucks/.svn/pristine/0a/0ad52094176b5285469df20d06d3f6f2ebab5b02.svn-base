@CHARSET "UTF-8";

body, html {
	margin: 0;
	padding: 0;
	background-color: #d8d8d8;
}

#leftSpace {
	width: 10%;
	height: 90px;
	background-color: #492525;
	float: left;
	border-top: 2px solid #f5c738;
}

#logoSpace {
	font-family: 'Ubuntu', sans-serif;
	font-size: 25pt;
	font-weight: bold;
	width: 60%;
	height: 90px;
	background-color: #492525;
	color: #f5c738;
	float: left;
	line-height: 90px;
	border-top: 2px solid #f5c738;
}

#logoSpace a {
	text-decoration: none;
	color: #f5c738;
}

#logoSpace a:hover {
	color: #f5c738;
}

#navSpace {
	width: 30%;
	height: 90px;
	background-color: #492525;
	float: left;
	border-top: 2px solid #f5c738;
}

#navSpace ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

#navSpace li {
	margin-top: 35px;
	margin-right: 20px;
	float: left;
}

#navSpace li a {
	text-decoration: none;
	color: white;
}

#navSpace li a:hover {
	color: #f5c738;
}

#middleSpace {
	width: 80%;
	float: left;
	margin-left: auto;
	margin-right: auto;
	padding: 30px;
	height:100%;
}

#nullSpace {
	width: 10%;
	float: left;
	height: 500px;
}

footer {
	width: 100%;
	height: 80px;
	background-color: #442222;
	margin-top: 0px;
	margin-bottom:0px;
	padding-bottom:0px;
	float: left;
	opacity:0.5;
	color:#d8d8d8;
	font-size:8pt;
	text-align: center;
}