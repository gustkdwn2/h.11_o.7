select * from member;
drop table member;

CREATE TABLE member(
memberID VARCHAR2(20) PRIMARY KEY,
memberPWD VARCHAR2(20),
memberName VARCHAR2(20),
memberEmail VARCHAR2(20),
memberPhone VARCHAR2(20),
)

